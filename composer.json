{
    "name": "wilma-digital/sylius-plugin-skeleton",
    "description": "Skeleton to create a Sylius plugin that impacts the frontend",
    "authors": [
        {
            "name": "WilMa Digital"
        },
        {
            "name": "Irene Iaccio"
        }
    ],
    "license": "MIT",
    "type": "sylius-plugin",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "WilmaDigital\\SyliusSkeletonPlugin\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "ergebnis/composer-normalize": true,
            "symfony/thanks": false
        },
        "sort-packages": true
    },
    "extra": {
        "sylius-plugin": {
            "class": "WilmaDigital\\SyliusSkeletonPlugin\\WilmaDigitalSkeletonPlugin"
        }
    }
}
